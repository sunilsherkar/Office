@model office.Models.AuthoritySignatory
@{
    Layout = null;
}

<div class="form-group">
    <label class="col-lg-2 control-label">
        Document Name
    </label>
    <div class="col-lg-4">
        @Html.DropDownList("DocumentID", new SelectList((System.Collections.IEnumerable)ViewData["MDocumentList"], "Value", "Text"), "Select Document", new { @class = "form-control", @id = "MDocumentID", Title = " " })
        <input type="hidden" value="@Model.selectedVal1" id="selectedVal1" />
        <input type="hidden" value="@Model.selectedVal2" id="selectedVal2" />
        <input type="hidden" value="@Model.selectedVal3" id="selectedVal3" />
        <input type="hidden" value="@Model.selectedVal4" id="selectedVal4" />
        <input type="hidden" value="@Model.selectedVal5" id="selectedVal5" />
        <input type="hidden" value="@Model.signatorId" id="signatorId" />
        <input type="hidden" value="@Model.SubRegistarOffice" id="hdnSubRegistarOffice" />
        <input type="hidden" value="@Model.either1" id="hdneither1" />
    </div>

</div>
<div class="form-group">
    <label class="col-lg-2 control-label">
        Area under developing
    </label>
    <div class="col-lg-4">
        <input id="Areaunderdeveloping" type="text" placeholder="Area Under Developing" value="@Model.AreaUnderDevelopment" class="form-control">
    </div>
    <label class="col-lg-2 control-label">Units</label>
    <div class="col-lg-4">
        @Html.DropDownList("AreaUnitID", new SelectList((System.Collections.IEnumerable)ViewData["UnitList"], "Value", "Text"), "Select Unit", new { @class = "form-control", @id = "AreaUnderProposalUnitId", Title = "AreaUnderProposalUnitId" })
    </div>
</div>
<div class="form-group">

    <label class="col-lg-2 control-label">
        Agreement No
    </label>
    <div class="col-lg-4">
        <input id="AgreementNo" type="text" placeholder="Agreement No" class="form-control" value="@Model.AgreementNo">
    </div>
    <label class="col-lg-2 control-label">
        Agreement Date
    </label>
    <div class="col-lg-4">
        @Html.TextBoxFor(m => m.AgreementDate, "{0:yyyy-MM-dd}", new { @class = "form-control", placeholder = "Agreement date", type = "date", id = "AgreementDate" })

        
    </div>
</div>


<script src="~/js/jquery-2.1.1.js"></script>
<link href="https://cdnjs.cloudflare.com/ajax/libs/select2/4.0.4/css/select2.min.css" rel="stylesheet" />
<script src="https://cdnjs.cloudflare.com/ajax/libs/select2/4.0.4/js/select2.min.js"></script>
<link href="~/css/select2.css" rel="stylesheet" />

