                var tdVals = $(this).text();
                idArrOffice.push(tdVals);
            });

            var SaveOfficeNo = [];
            for (var i = 0; i < idArrOffice.length; i++) {
                var arrElement = idArrOffice[i].split("~");
                item = {}
                item["OfficeNo"] = arrElement[0];
                SaveOfficeNo.push(item);
