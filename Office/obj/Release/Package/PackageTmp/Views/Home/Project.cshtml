                                <option value="2">Miss.</option>
                                <option value="3">Dr.</option>
                                <option value="4">Ms.</option>
                            </select>
                            <i class="fa fa-plus" id="addPerson" aria-hidden="true" style="margin-left: 8px; margin-top: 9px;"></i>
                        </div>
