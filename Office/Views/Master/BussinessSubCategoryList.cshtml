@{
    ViewBag.Title = ""; 
} 
<div class="row">
    <script src="~/js/jquery-2.1.1.js"></script> 
    <div class="col-lg-12">
        <div id="AddNew">  
        </div>
        <div class="ibox float-e-margins">
            <div class="ibox-title">
                <div class="row" id="AddNew">
                    </div>
                </div>
                <div class="ibox-content"> 
                    <div class="bindGrid">
                        <div id="mgrid"> 
                        </div>
                    </div>
                </div>
            </div>
    </div>
</div>
<script>
    $(document).ready(function () {

          $.ajax({
                url: "@Url.Action("AddBussinessSubCategory", "Master")",
                contentType: "application/json",

                dataType: "text",
                type: "POST",
                async: true,
                success: function (data) {
                    $('#AddNew').html(data);

                },
                error: function (data) {
                },
                complete: function () {
                }
            }); 
          $.ajax({
                url: "@Url.Action("LoadBussinessSubCategoryGrid", "Master")",
                contentType: "application/json",

                dataType: "text",
                type: "Get",
                async: true,
                success: function (data) {
                    $('#mgrid').html(data);

                },
                error: function (data) {
                },
                complete: function () {
                }
            });  
    }); 
</script>