@model office.Models.TemplateCompanyTeam
@{
    Layout = null;
}
 
<div class="row">
    <div class="col-lg-12">
        <h5 class="help-block m-b-none">Internal Team </h5>
        <hr style="margin-top: 11px; margin-bottom: 0px;" />
    </div>
    <div>
        <div class="DivInternalTeam">
            <table class="table" id="ExternalTeamlist" style="margin-bottom: 0px!important">
                <tbody>
                    @if (Model.SaveInternalTeam != null)
                    {
                        foreach (var item4 in Model.SaveInternalTeam)
                        {
                            <tr>
                                <td>
                                    
                                        @if (item4.isselected == 1)
                                        {
                                            <input type="radio" name="iteam" class="i-checks chkCInternalTeam" v="@item4.internalTeamid" divName="checkboxDiv_@item4.internalTeamid" checked="checked">
                                        }
                                        else
                                        {
                                            <input type="radio" name="iteam" class="i-checks chkCInternalTeam" v="@item4.internalTeamid" divName="checkboxDiv_@item4.internalTeamid">
                                        }
                                     
                                </td>
                                <td> @item4.InternalPersonName</td>
                            </tr>
                        }
                    }
                </tbody>
            </table>
             
        </div>
    </div>
</div>

<div class="row">
     
    <div class="col-lg-12">
        <hr style="margin-top: 11px; margin-bottom: 0px;" />
        <h5 class="help-block m-b-none">External Team </h5>
        <hr style="margin-top: 11px; margin-bottom: 0px;" />
    </div>

    <div class="DivExternalTeam">

        <table class="table" id="ExternalTeamlist" style="margin-bottom: 0px!important">
            <tbody>
                @if (Model.SaveExternalTeam != null)
                {
                    foreach (var item4 in Model.SaveExternalTeam)
                    {
                        <tr>
                            <td>
                                 
                                    @if (item4.isselected == 1)
                                    {
                                        <input type="radio" name="companyEteam" class="i-checks chkCExternalTeam" v="@item4.ExternalTeamid" divName="checkboxDiv_@item4.ExternalTeamid" checked="checked" />
                                    }
                                    else
                                    {
                                        <input type="radio" name="companyEteam" class="i-checks chkCExternalTeam" v="@item4.ExternalTeamid" divName="checkboxDiv_@item4.ExternalTeamid" />
                                    }
                               
                            </td>
                            <td> @item4.ExternalPersonName</td>
                        </tr>
                    }
                }
            </tbody>
        </table>
    </div>
</div>
  

<div class="row">
    <div class="col-lg-12">
        <hr style="margin-top: 11px; margin-bottom: 0px;" />
        <h5 class="help-block m-b-none">Company Address </h5>
        <hr style="margin-top: 11px; margin-bottom: 0px;" />
    </div>
    <div>
        <div class="DivInternalTeam">
            <table class="table" id="Addresslist" style="margin-bottom: 0px!important">

                <tbody>
                    @if (Model.CompanyAddress != null)
                    {
                        foreach (var item4 in Model.CompanyAddress)
                        {
                            <tr>
                                <td>
                                    <div class="" style="margin-left: 0!important">

                                        @if (item4.isselected == 1)
                                        {
                                            <input type="radio" name="address" class="i-checks chkCAddress" v="@item4.AddressID" divName="checkboxDiv_@item4.AddressID" checked="checked">
                                        }
                                        else
                                        {
                                            <input type="radio" name="address" class="i-checks chkCAddress" v="@item4.AddressID" divName="checkboxDiv_@item4.AddressID">
                                        }

                                    </div>
                                </td>
                                
                                <td> @item4.Address1</td>

                            </tr>
                        }
                    }
                </tbody>
            </table>
            
            
        </div>
    </div>
</div>