                                        <div class="full-height-scroll">
                                            <div class="bindGrid">
                                                <div class="form-group">
                                                    <div>
                                                        <div class="col-lg-3">
                                                            <span class="IsultiOwner">

                                                                @Html.CheckBox("Propertycard1", new { @class = "", title = "Is 7/12 ", @id = "Propertycard1" })
                                                                <label for="checkbox5">
                                                                    7-12
                                                                </label>
                                                            </span>
                                                        </div>
                                                        <div class="col-lg-3">
                                                            <span class="IsultiOwner">
                                                                @Html.CheckBox("Propertycard2", new { @class = "", title = "Is Property Card", @id = "Propertycard2" })
                                                                <label for="checkbox5">
                                                                    PropertyCard
                                                                </label>
                                                            </span>
                                                        </div>
                                                        <div class="col-lg-3">
                                                            <span class="">
                                                                <label for="checkbox5">
                                                                    Pre Proposed data
                                                                </label>
                                                            </span>

                                                        </div>
                                                        <div class="col-lg-3">
                                                            @Html.DropDownList("SurveyNoTypeID1", new SelectList((System.Collections.IEnumerable)ViewData["PreProposalSurveyList"], "Value", "Text"), "Select Type", new { @class = "form-control", @id = "PreproSurveyList", Title = "SurveyNoTypeID" })

                                                        </div>
                                                    </div>
                                                </div>

                                                <div id="SurveyPartial">
                                                    <div class="form-group disabledDiv" id="SurveyNo">
