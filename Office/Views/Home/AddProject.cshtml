                        <div class="col-lg-4">
                            <input id="Enddate" type="date" placeholder="EndDate" class="form-control">
                        </div>

                    </div>


                    <div class="form-group">
                        <label class="col-lg-2 control-label">Consultant Type</label>
                        <div class="col-lg-4" style="display:inline-flex;">
                            @Html.DropDownList("ConsultantTypeId", new SelectList((System.Collections.IEnumerable)ViewData["ConsultantTypeList"], "Value", "Text"), "Select ConsultantType", new { @class = "form-control", @id = "ConsultantTypeId", Title = "ConsultantTypeId" })
                        </div>
                        <label class="col-lg-2 control-label">Consultant</label>
                        <div class="col-lg-4" style="display:inline-flex;">
                            @Html.DropDownList("ConsultantId", new SelectList((System.Collections.IEnumerable)ViewData["ConsultantList"], "Value", "Text"), "Select Consultant", new { @class = "form-control", @id = "ConsultantId", Title = "ConsultantId" })
