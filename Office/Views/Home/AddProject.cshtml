
@model office.Models.SaveProject
@using PagedList;
@using PagedList.Mvc;
@using GridMvc.Html

@{
    ViewBag.Title = "Project";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<script src="~/js/jquery-2.1.1.js"></script>
<div class="row">
    <div class="col-lg-8">
        <div class="ibox float-e-margins">
            <div class="ibox-title">
                <h5>Project Information</h5>

            </div>
            <div class="ibox-content">
                <form class="form-horizontal">
                    
                    <div class="form-group">
                        <label class="col-lg-2 control-label">Enquiry Date</label>
                        <div class="col-lg-10">
                            <input type="date" id="EnquiryDate" value="@DateTime.Now.ToString("yyyy-MM-dd")" placeholder="EnquiryDate" class="form-control">
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-lg-2 control-label">Name</label>
                        <div class="col-lg-10"><input type="text" id="ProjectName" placeholder="ProjectName" class="form-control"></div>
                    </div>

                    <div class="form-group">
                        <label class="col-lg-2 control-label">Short Name</label>
                        <div class="col-lg-10"><input type="text" id="ShortName" placeholder="Short Name" class="form-control"></div>
                    </div>

                    <div class="form-group">
                        <label class="col-lg-2 control-label">Developers</label>

                        <div class="col-lg-4" style="display:inline-flex;">
                            @Html.DropDownList("DeveloperId", new SelectList((System.Collections.IEnumerable)ViewData["CompanyNameList"], "Value", "Text"), "Select Company", new { @class = "form-control", @id = "DeveloperId", Title = "DeveloperId" })
                        </div>
                        <label class="col-lg-2 control-label">Relation</label>

                        <div class="col-lg-4" style="display:inline-flex;">
                            @Html.DropDownList("OwnershipId", new SelectList((System.Collections.IEnumerable)ViewData["OwnershipTypeList"], "Value", "Text"), "Select Relation", new { @class = "form-control", @id = "OwnershipId", Title = "OwnershipId" })
                            <i class="fa fa-plus" id="addDeveloper" aria-hidden="true" style="margin-left: 8px; margin-top: 9px;"></i>
                        </div>
                    </div>


                    <div class="form-group">
                        <label class="col-lg-2 control-label">Conact Person</label>
                        <div class="col-lg-10" style="display:inline-flex;">
                            @Html.DropDownList("ConactDeveloperPersonId", new SelectList((System.Collections.IEnumerable)ViewData["ConactPersonList"], "Value", "Text"), "Select Contact Person", new { @class = "form-control", @id = "ConactDeveloperPersonId", Title = "ConactDeveloperPersonId" })
                            <i class="fa fa-plus" id="addDeveloperContactPerson" aria-hidden="true" style="margin-left: 8px; margin-top: 9px;"></i>
                        </div>
                        <span style="margin-left: 17px;color: darkgray;font-size: smaller;">Developer Side</span>
                    </div>

                    <div class="form-group">
                        <label class="col-lg-2 control-label">Coordinator</label>

                        <div class="col-lg-10" style="display:inline-flex;">
                            @Html.DropDownList("CoordinatorId", new SelectList((System.Collections.IEnumerable)ViewData["ConactPersonList"], "Value", "Text"), "Select Coordinator", new { @class = "form-control", @id = "CoordinatorId", Title = "CoordinatorId" })
                            <i class="fa fa-plus" id="addCoordinator" aria-hidden="true" style="margin-left: 8px; margin-top: 9px;"></i>
                        </div>
                    </div>

                    <div class="form-group">
                        <label class="col-lg-2 control-label">Assistant</label>

                        <div class="col-lg-10" style="display:inline-flex;">
                            @Html.DropDownList("AssistantId", new SelectList((System.Collections.IEnumerable)ViewData["ConactPersonList"], "Value", "Text"), "Select Assistant", new { @class = "form-control", @id = "AssistantId", Title = "AssistantId" })
